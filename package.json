{
  "name": "@angular-material-extensions/faq",
  "description": "Angular Library built with material design in order to provide a reusable faq (frequently asked questions) component for every project. Ask, Answer and List",
  "version": "3.0.1",
  "homepage": "https://github.com/angular-material-extensions/faq",
  "author": {
    "name": "anthonynahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/angular-material-extensions/faq.git"
  },
  "license": "MIT",
  "keywords": [
    "ngx",
    " angular",
    " library",
    " faq",
    " material",
    " asking",
    " questions",
    " answers",
    "material extensions",
    "material design"
  ],
  "bugs": {
    "url": "https://github.com/angular-material-extensions/faq/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=angular-material-extensions/faq --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/flex-layout": "6.0.0-beta.17",
    "@angular/forms": "^6.1.6",
    "@angular/material": "^6.4.7"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/platform-server": "^6.1.6",
    "@compodoc/gulp-compodoc": "^0.0.9",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "angular-cli-ghpages": "0.5.2",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^9.1.3",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^4.4.4",
    "commitplease": "3.2.0",
    "conventional-github-releaser": "2.0.2",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-conventional-changelog": "^2.0.3",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "^2.8.0",
    "gulp-hub": "0.8.0",
    "gulp-inline-ng2-template": "4.1.0",
    "gulp-tslint": "^8.1.3",
    "gulp-util": "3.0.8",
    "html-loader": "0.5.5",
    "is-docker": "1.1.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jest-preset-angular": "^5.2.3",
    "lodash": "4.17.10",
    "node-sass": "^4.9.3",
    "postcss": "^7.0.2",
    "postcss-strip-inline-comments": "0.1.5",
    "pump": "3.0.0",
    "raw-loader": "0.5.1",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.1",
    "rxjs": "6.2.1",
    "sass-loader": "7.0.3",
    "source-map-loader": "0.2.3",
    "to-string-loader": "1.1.5",
    "travis-status": "^3.0.1",
    "tslint": "~5.11.0",
    "typescript": "2.9.2",
    "yargs": "11.0.0",
    "zone.js": "0.8.26"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@types/jasmine",
      "@types/jest",
      "@types/node",
      "conventional-github-releaser",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
